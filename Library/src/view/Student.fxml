<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="684.0" prefWidth="609.0"
            xmlns="http://javafx.com/javafx/10.0.1" fx:controller="ControllerLibrary.StudentController">
    <children>
        <ImageView fx:id="lblBack" fitHeight="40.0" fitWidth="35.0" layoutX="26.0" layoutY="24.0"
                   onMouseClicked="#lblBack_Clicked" pickOnBounds="true" preserveRatio="true"
                   AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0">
            <image>
                <Image url="@../images/guest_male_96px.png"/>
            </image>
        </ImageView>
        <JFXTextField fx:id="txtStudentID" labelFloat="true" layoutX="30.0" layoutY="100.0"
                      onAction="#txtStudentID_Action" prefHeight="29.0" prefWidth="411.0" promptText="Student ID"
                      AnchorPane.leftAnchor="30.0">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXTextField>
        <JFXTextField fx:id="txtName" labelFloat="true" layoutX="20.0" layoutY="160.0" onAction="#txtName_Action"
                      prefHeight="29.0" prefWidth="411.0" promptText="Name" AnchorPane.leftAnchor="30.0">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXTextField>
        <JFXTextField fx:id="txtGender" labelFloat="true" layoutX="20.0" layoutY="280.0" onAction="#txtGender_Action"
                      prefHeight="29.0" prefWidth="411.0" promptText="Gender" AnchorPane.leftAnchor="30.0">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXTextField>
        <JFXTextField fx:id="txtBirthday" labelFloat="true" layoutX="20.0" layoutY="220.0"
                      onAction="#txtBirthday_Action" prefHeight="29.0" prefWidth="411.0" promptText="BirthDay"
                      AnchorPane.leftAnchor="30.0">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXTextField>
        <JFXTextField fx:id="txtDepartment" labelFloat="true" layoutX="30.0" layoutY="340.0"
                      onAction="#txtDepartment_Action" prefHeight="29.0" prefWidth="411.0" promptText="Department">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXTextField>
        <TableView fx:id="tblStudent" layoutX="30.0" layoutY="417.0" prefHeight="234.0" prefWidth="547.0">
            <columns>
                <TableColumn prefWidth="75.0" text="Student ID"/>
                <TableColumn prefWidth="193.0" text="Name"/>
                <TableColumn prefWidth="65.0" text="BirthDay"/>
                <TableColumn prefWidth="51.0" text="Gender"/>
                <TableColumn prefWidth="92.0" text="Department ID"/>
            </columns>
        </TableView>
        <Label fx:id="lblNewStudent" alignment="CENTER" layoutX="400.0" layoutY="32.0" onMouseClicked="#lblNewStudent"
               prefHeight="20.0" prefWidth="106.0" text="New Student" textFill="#5e10da" AnchorPane.rightAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <JFXButton fx:id="btnAdd" layoutX="491.0" layoutY="189.0" onAction="#btnAdd_Action" prefHeight="25.0"
                   prefWidth="70.0" text="Add" textFill="#5e10da"/>
        <Label alignment="CENTER" layoutX="95.0" layoutY="32.0" prefHeight="17.0" prefWidth="120.0"
               text="Student Details" textFill="#5e10da">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
